# This was inserted by `prisma init`:
# Environment variables declared in this file are automatically made available to Prisma.
# See the documentation for more detail: https://pris.ly/d/prisma-schema#accessing-environment-variables-from-the-schema

# Prisma supports the native connection string format for PostgreSQL, MySQL, SQLite, SQL Server, MongoDB and CockroachDB.
# See the documentation for all the connection string options: https://pris.ly/d/connection-strings

# Database URL
DATABASE_URL="postgresql://prisma:secret@postgres:5432/dbdata?schema=public"

# Upload dir
UPLOAD_DIR="./image_dir"

# this is our jason web token secret hash
JWT_SECRET="super_secret_jwt_token"

# Token expiration time
ACCESS_TOKEN_EXPIRATION="300m"
REFRESH_TOKEN_EXPIRATION="1y"

# App name for Authenticator
MY_2FA_APP_NAME="42 APP NAME"

# Site URL
SITE_URL="http://localhost"

# PORTS
PORT=4000
FRONT_PORT=3000

# URL
FRONT_URL="http://localhost:3000"

# Environment
# ENVIRONMENT="PRODUCTION"
ENVIRONMENT="DEVELOPMENT"

# 42 API
FORTYTWO_ID="your_42_id_here"

FORTYTWO_SECRET="your_42_secret_here"

FORTYTWO_CALLBACK="your_api_callback here"